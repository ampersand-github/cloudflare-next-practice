{
	"name": "cloudflare-next-practice",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"vercel-build": "next build",
		"tsc:watch": "tsc --watch --noEmit",
		"local": "next dev -p 3100",
		"biome": "npx @biomejs/biome check --write src",
		"pages:build": "npx @cloudflare/next-on-pages",
		"preview": "npm run pages:build && wrangler pages dev",
		"deploy": "npm run pages:build && wrangler pages deploy",
		"drizzle:generate": "npx drizzle-kit generate",
		"d1:migrate:local": "npx wrangler d1 migrations apply my-next-app-db --local",
		"d1:migrate:prod": "npx wrangler d1 migrations apply my-next-app-db --remote"
	},
	"dependencies": {
		"@auth/core": "^0.32.0",
		"@auth/drizzle-adapter": "^1.2.0",
		"@hono/auth-js": "^1.0.8",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-slot": "^1.0.2",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.31.1",
		"hono": "^4.4.3",
		"jotai": "^2.8.3",
		"next": "14.2.3",
		"next-auth": "^5.0.0-beta.18",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"tailwind-merge": "^2.3.0",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.0",
		"@cloudflare/next-on-pages": "^1.11.3",
		"@cloudflare/workers-types": "^4.20240603.0",
		"@types/node": "^20.14.2",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"autoprefixer": "^10.4.19",
		"drizzle-kit": "^0.22.2",
		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.3",
		"typescript": "^5.4.5",
		"vercel": "^34.2.5",
		"wrangler": "^3.59.0"
	}
}
